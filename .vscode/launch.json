{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Poetry: Debug Scrape Script",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "module": "poetry",
            "args": [
                "run",
                "scrape"
            ],
            "justMyCode": true
        }
    ]
}